// VOID
// SYMCRYPT_CALL
// SymCryptWipe( _Out_writes_bytes_( cbData )   PVOID  pbData,
//                                        SIZE_T cbData )

SymCryptWipeAsm: .global SymCryptWipeAsm
.type SymCryptWipeAsm, %function
// .func SymCryptWipeAsm
push {lr}
        // we just jump to memset.
        // this is enough to stop the compiler optimizing the memset away.
        mov     r2, r1
        mov     r1, #0
        bl       memset
pop {pc}
// .endfunc // SymCryptWipeAsm